{
  "name": "@utils/app",
  "version": "1.0.1",
  "description": "Utilities for doing various annoying tasks",
  "main": "node_modules/expo/AppEntry.js",
  "repository": "https://github.com/Xenfo/utils.git",
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "MIT",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider expo start",
    "android": "NODE_OPTIONS=--openssl-legacy-provider expo start --android",
    "ios": "NODE_OPTIONS=--openssl-legacy-provider expo start --ios",
    "web": "NODE_OPTIONS=--openssl-legacy-provider expo start --web",
    "eject": "NODE_OPTIONS=--openssl-legacy-provider expo eject",
    "lint": "run lint:eslint && run lint:prettier",
    "lint:eslint": "run r:eslint --fix --ignore-path ../../.gitignore --ext .js,.ts,.tsx .",
    "lint:prettier": "run r:prettier --write --ignore-path ../../.gitignore ."
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@utils/lib": "workspace:^",
    "expo": "~44.0.0",
    "expo-clipboard": "~2.1.0",
    "expo-status-bar": "~1.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-43.0.0.tar.gz",
    "react-native-safe-area-context": "3.4.0",
    "react-native-screens": "~3.11.0",
    "react-native-web": "~0.17.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@expo/metro-config": "^0.3.5",
    "@types/react": "~17.0.37",
    "@types/react-native": "~0.66.8",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0"
  }
}
